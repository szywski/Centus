<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="260.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Centus.NewAccountScene">
   <children>
      <Button fx:id="addBtn" layoutX="260.0" layoutY="203.0" mnemonicParsing="false" onAction="#addAccount" prefHeight="26.0" prefWidth="58.0" text="Add" />
      <Button fx:id="exitBtn" layoutX="327.0" layoutY="203.0" mnemonicParsing="false" prefHeight="26.0" prefWidth="58.0" text="Exit" />
      <PasswordField fx:id="passwordL" layoutX="126.0" layoutY="86.0" promptText="Password">
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </PasswordField>
      <PasswordField fx:id="repeatPasswordL" layoutX="126.0" layoutY="130.0" promptText="Repeat password">
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </PasswordField>
      <TextField fx:id="loginL" alignment="BOTTOM_LEFT" layoutX="125.0" layoutY="44.0" promptText="Login">
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </TextField>
      <Label fx:id="statusL" layoutX="43.0" layoutY="169.0" prefHeight="27.0" prefWidth="330.0" textFill="#949494">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
